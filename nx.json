            "dependencies": "*",
            "devDependencies": "*"
        },
        ".eslintrc.json": "*"
    },
    "tasksRunnerOptions": {
        "default": {
            "runner": "@nrwl/nx-cloud",
            "options": {
                "cacheableOperations": ["build", "lint", "test", "e2e"],
                "accessToken": "ODY5NDM2NTQtNmZhNC00MzIxLTk2OTctMDVjNjAzZjdiODg4fHJlYWQtd3JpdGU="
            }
        }
    },
    "targetDependencies": {
        "build": [{
            "target": "build",
